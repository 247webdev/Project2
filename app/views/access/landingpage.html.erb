<h1>Landing Page</h1>

<h2><%= link_to "Signup with Email", new_user_path %></h2>

<h2>Login</h2>
<%= form_tag(login_path) do %>

  <%= label_tag(:username) %>
  <%= text_field_tag(:username, params[:username]) %>

  <%= label_tag(:password) %>
  <%= password_field_tag(:password) %>

  <%= submit_tag("Log in", class: "btn btn-primary") %>
  <% end %>
